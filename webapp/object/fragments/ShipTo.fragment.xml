<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:core="sap.ui.core"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:m="sap.m">
	<ValueHelpDialog
        id="idShipToVH"
		title="Ship To"
		ok=".onShipToVHOkPress"
		cancel=".onValueHelpCancelPress"
		afterClose=".onValueHelpAfterClose"
		supportMultiselect="false"
		key="Kunnr"
	>
    	<filterBar>
			<fb:FilterBar id="_IDGenFilterBar1" advancedMode="true" search=".onSearchShipTo"  >
				<fb:filterGroupItems>
					<fb:FilterGroupItem id="_IDGenFilterGroupItem1"
						groupName="__$INTERNAL$"
						name="Bstkd"
						label="Search Term"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input id="_IDGenInput1" name="Sortl" value="{ShipToFilter>/Sortl}" />
						</fb:control>
					</fb:FilterGroupItem>

					<fb:FilterGroupItem id="_IDGenFilterGroupItem2"
						groupName="__$INTERNAL$"
						name="Vkorg"
						label="Sales Org"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input id="_IDGenInput2" name="Vkorg" value="{ShipToFilter>/Vkorg}"/>
						</fb:control>
					</fb:FilterGroupItem>

					<fb:FilterGroupItem id="_IDGenFilterGroupItem3"
						groupName="__$INTERNAL$"
						name="Kunnr"
						label="Country"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input id="_IDGenInput3" name="Land1" value="{ShipToFilter>/Land1}"/>
						</fb:control>
					</fb:FilterGroupItem>

					<fb:FilterGroupItem id="_IDGenFilterGroupItem4"
							groupName="__$INTERNAL$"
							name="Vtweg"
							label="Distribution Channel"
							visibleInFilterBar="true"
						>
						<fb:control>
							<m:Input id="_IDGenInput4" name="Vtweg" value="{ShipToFilter>/Vtweg}"/>
						</fb:control>
					</fb:FilterGroupItem>

					<fb:FilterGroupItem id="_IDGenFilterGroupItem5"
							groupName="__$INTERNAL$"
							name="Spart"
							label="Division"
							visibleInFilterBar="true"
						>
						<fb:control>
							<m:Input id="_IDGenInput5" name="Spart" value="{ShipToFilter>/Spart}"/>
						</fb:control>
					</fb:FilterGroupItem>

                    <fb:FilterGroupItem id="_IDGenFilterGroupItem6"
							groupName="__$INTERNAL$"
							name="Vkbur"
							label="Sales office"
							visibleInFilterBar="true"
						>
						<fb:control>
							<m:Input id="_IDGenInput6" name="Vkbur" value="{ShipToFilter>/Vkbur}" />
						</fb:control>
					</fb:FilterGroupItem>

                    <fb:FilterGroupItem id="_IDGenFilterGroupItem7"
							groupName="__$INTERNAL$"
							name="Vkgrp"
							label="Sales group"
							visibleInFilterBar="true"
						>
						<fb:control>
							<m:Input id="_IDGenInput7" name="Vkgrp" value="{ShipToFilter>/Vkgrp}"/>
						</fb:control>
					</fb:FilterGroupItem>

                        <fb:FilterGroupItem id="_IDGenFilterGroupItem8"
							groupName="__$INTERNAL$"
							name="Pstlz"
							label="Postal Code"
							visibleInFilterBar="true"
						>
						<fb:control>
							<m:Input id="_IDGenInput8" name="Pstlz" value="{ShipToFilter>/Pstlz}" />
						</fb:control>
					</fb:FilterGroupItem>

                       <fb:FilterGroupItem id="_IDGenFilterGroupItem9"
							groupName="__$INTERNAL$"
							name="Auart"
							label="Sales Doc. Type"
							visibleInFilterBar="true"
						>
						<fb:control>
							<m:Input id="_IDGenInput9" name="Auart" value="{ShipToFilter>/Auart}" />
						</fb:control>
					</fb:FilterGroupItem>

                    <fb:FilterGroupItem id="_IDGenFilterGroupItem10"
							groupName="__$INTERNAL$"
							name="Mcod3"
							label="City"
							visibleInFilterBar="true"
						>
						<fb:control>
							<m:Input id="_IDGenInput10" name="Mcod3" value="{ShipToFilter>/Mcod3}" />
						</fb:control>
					</fb:FilterGroupItem>

                     <fb:FilterGroupItem id="_IDGenFilterGroupItem11"
							groupName="__$INTERNAL$"
							name="Trvog"
							label="Name"
							visibleInFilterBar="true"
						>
						<fb:control>
							<m:Input id="_IDGenInput11" name="Mcod1" value="{ShipToFilter>/Mcod1}"/>
						</fb:control>
					</fb:FilterGroupItem>

                     <fb:FilterGroupItem id="_IDGenFilterGroupItem12"
							groupName="__$INTERNAL$"
							name="Vbeln"
							label="Customer"
							visibleInFilterBar="true"
						>
						<fb:control>
							<m:Input id="_IDGenInput12" name="Kunnr" value="{ShipToFilter>/Kunnr}" />
						</fb:control>
					</fb:FilterGroupItem>

				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
