<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:form="sap.ui.layout.form">
<form:Form id="idQuickHeaderForm" editable="true"  binding="{QuotedetailModel>/QUOTEHEADER}" >
		<form:layout>
			<form:ResponsiveGridLayout id="idQuickHeaderFormLayout"
            	labelSpanXL="4"
					labelSpanL="4"
					labelSpanM="12"
					labelSpanS="12"
					emptySpanXL="0"
					emptySpanL="0"
					emptySpanM="0"
					emptySpanS="0"
					columnsXL="2"
					columnsL="2"
					columnsM="2" />
		</form:layout>  
		<form:title>
			<core:Title id="idQuickHeaderFormTitle" text="Quick Header Details"/>
		</form:title>
		<form:formContainers>
			<form:FormContainer id="idQuickHeaderFormContainer">
                <form:FormElement  label="Qt.Name">
					<form:fields>
	                   <Input value="{QuotedetailModel>QtName}" tooltip="{QuotedetailModel>QtName}" placeholder="Add Quote Name"/>
                       
					</form:fields>
				</form:FormElement>
				<form:FormElement  label="Qt.Email">
					<form:fields>
	                   <Input value="{QuotedetailModel>QtEmail}" tooltip="{QuotedetailModel>QtEmail}" placeholder="Add Quote Email"/>
                       
					</form:fields>
				</form:FormElement>
                <!-- <form:FormElement  label="Order Combo">
					<form:fields>
	                   <CheckBox selected="{QuotedetailModel>Kzazu}" />
                      
					</form:fields>
				</form:FormElement> -->
                <!-- <form:FormElement  label="Incoterms 1">
					<form:fields>
	                   <Input value="{QuotedetailModel>Inco1}" showValueHelp="true" valueHelpRequest="onIncoTerms1VH" />
                       
                       
					</form:fields>
				</form:FormElement> -->
                <!-- </form:FormContainer>
                 <form:FormContainer> -->
                   <form:FormElement  label="Mat.Source">
					<form:fields>
	                   <Input id="idMaterialSource" selectedKey="{QuotedetailModel>HeaderZzmatSrc}"  showValueHelp="true" valueHelpRequest="onMaterialSourceVH"/>
                    
					</form:fields>
				</form:FormElement>
                </form:FormContainer>
                <form:FormContainer id="idQuickHeaderFormContainer2">
                <form:FormElement  label="Plant">
					<form:fields>
	                   <Input id="idQuickHeaderPlant" value="{QuotedetailModel>HeaderWerks}"  showValueHelp="true" valueHelpRequest="onPlantVH" />
                    
					</form:fields>
				</form:FormElement>
                <form:FormElement  label="Incoterms 1">
					<form:fields>
	                   <Input value="{QuotedetailModel>Inco1}" showValueHelp="true" valueHelpRequest="onIncoTerms1VH" />
                       
                       
					</form:fields>
				</form:FormElement>
                <form:FormElement  label="Incoterms 2">
					<form:fields>
	                  
                        <Input value="{QuotedetailModel>Inco2L}" tooltip="{QuotedetailModel>Inco2L}" />
                       
					</form:fields>
				</form:FormElement>
                 </form:FormContainer>
                
		</form:formContainers>
	</form:Form>

</core:FragmentDefinition>