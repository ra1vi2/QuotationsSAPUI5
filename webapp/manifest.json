{
  "_version": "1.28.0",
  "sap.app": {
    "id": "com.quote.quotations",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZOTC_FIORI_QUOTE_SRV/",
        "type": "OData",
        "settings": {
          "annotations": ["localAnnotations"],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "localAnnotations": {
        "type": "ODataAnnotation",
        "uri": "annotations/annotations.xml",
        "settings": {
          "localUri": "annotations/annotations.xml"
        }
      },
      "freightService": {
        "uri": "/sap/opu/odata/sap/ZOTC_FIORI_QUOT_FREIGHT_EST_SRV/",
        "type": "OData",
        "settings": {
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.84.7",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.quote.quotations.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "defaultOperationMode": "Server",
          "defaultCountMode": "Inline",
          "defaultBindingMode": "TwoWay"
        }
      },
      "freight": {
        "dataSource": "freightService",
        "preload": true,
        "settings": {
          "defaultOperationMode": "Server",
          "defaultCountMode": "Inline",
          "defaultBindingMode": "TwoWay"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.quote.quotations",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteQuoteMain",
          "pattern": "",
          "target": ["TargetQuoteMain"]
        },
        {
          "name": "detail",
          "pattern": "detail",
          "target": ["TargetQuoteDetail"]
        }
      ],
      "targets": {
        "TargetQuoteMain": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "view.QuoteMain",
          "viewName": "view.QuoteMain"
        },
        "TargetQuoteDetail": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "object.QuoteDetail",
          "viewName": "object.QuoteDetail"
        }
      }
    },
    "rootView": {
      "viewName": "com.quote.quotations.view.QuoteMain",
      "type": "XML",
      "async": true,
      "id": "QuoteMain"
    }
  }
}
